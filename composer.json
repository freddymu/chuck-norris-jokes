{
    "name": "freddymu/chuck-norris-jokes",
    "description": "Create random Chuck Norris jokes",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Freddy",
        "email": "fredhopelane@gmail.com"
    }],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Freddymu\\ChuckNorrisJokes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freddymu\\ChuckNorrisJokes\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freddymu\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Freddymu\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}